<tr data-matricula="{{#if aluno.usuario}}{{aluno.usuario.matricula}}{{else}}{{aluno.matricula}}{{/if}}">
    <td scope="row">{{#if aluno.usuario}}{{aluno.usuario.matricula}}{{else}}{{aluno.matricula}}{{/if}}</td>
    <td>{{#if aluno.usuario}}{{aluno.usuario.nome}}{{else}}{{aluno.nome}}{{/if}}</td>
    <td class="text-center">
        <button class="btn btn-default btn-xs edit">
            <i class="fa fa-pencil-square-o"></i> {{lang 'general.edit'}}
        </button>

        <button class="btn btn-danger btn-xs remove">
            <i class="fa fa-remove"></i> {{lang 'general.remove'}}
        </button>
    </td>
</tr>